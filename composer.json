{
    "name": "matks/xmlrumble",
    "description": "Xml to Yaml buddy translator",
    "type": "library",
    "keywords": [
        "xml",
        "yaml",
        "converter"
    ],
    "homepage": "https://github.com/matks/XMLRumble",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Mathieu Ferment",
            "email": "mathieu.ferment@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/yaml": "~2.0",
        "suramon/simple-xml-reader": "dev-master",
        "symfony/finder": "~2.0"
    },
    "require-dev": {
        "atoum/atoum" : "dev-master"
    },
    "autoload": {
        "psr-0" : {
            "Matks\\Rumble" : "src/",
            "Matks\\RumbleTest" : "tests/Functional/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
